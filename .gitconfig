[core]
	editor = nano
	excludesfile = /Users/ted/.global_gitignore
[user]
	name = Ted
	email = cycloss@users.noreply.github.com
	username = cycloss
[merge]
    	tool = vscode
[mergetool "vscode"]
    	cmd = code -n --wait $MERGED
[diff]
    	tool = vscode
[difftool "vscode"]
    	cmd = code -n --wait --diff $LOCAL $REMOTE
[init]
	defaultBranch = main
